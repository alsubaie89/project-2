    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_pack_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  
  </head>

  <body>
  

  <nav class="navbar navbar-expand-lg navbar-light bg-light" id="bb">
   <%= image_tag 'logo2.png', class: "logo2" %>
   
  
  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>
  <div class="collapse navbar-collapse" id="navbarSupportedContent">
    <ul class="navbar-nav mr-auto">
      <li class="nav-item active">
        <a class="nav-link" href="/cars">Home <span class="sr-only">(current)</span></a>
      </li>
      <% if(user_signed_in?)%>
          <li class="nav-item">
            <a class="nav-link" href="/cars/new">Create New</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="/my-cars">Your cars </a>
          </li>
          <li class="nav-item">
            <%= link_to "Sign Out", destroy_user_session_path, method: :delete , :class => "nav-link"%>
          </li>
          <li class="nav-item">
          <%= link_to "Update my Account", edit_user_registration_path,:class => "nav-link" %>
          </li>
       <% else %>
       <li class="nav-item">
          <%= link_to "Sign in", new_user_session_path ,:class => "nav-link"%>
          </li>
              <li class="nav-item">
                    <%= link_to "Sign Up", new_user_registration_path,:class => "nav-link" %>
              </li>
       <% end %>
    </ul>
   </header>
  </div>
  <p class="notice"><%= notice %></p>
   <p class="alert"><%= alert %></p>
</nav>
<br>

   <script
  src="https://code.jquery.com/jquery-3.4.1.min.js"
  integrity="sha256-CSXorXvZcTkaix6Yvo6HppcZGetbYMGWSFlBw8HfCJo="
  crossorigin="anonymous"></script>
  <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
